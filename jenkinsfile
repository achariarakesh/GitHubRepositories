pipeline {
agent any
  stages {
    stage('Build') {
      steps {
        bat "NuGet.exe restore TestWebSolution.sln"
        bat "\"${tool 'MSBuildLocal'}\" TestWebSolution.sln /p:DeployOnBuild=true /p:DeployDefaultTarget=WebPublish /p:WebPublishMethod=FileSystem /p:SkipInvalidConfigurations=true /t:build /p:Configuration=Release /p:Platform=\"Any CPU\" /p:DeleteExistingFiles=True /p:publishUrl=c:\\inetpub\\wwwroot"
      }
    }
  }
}
